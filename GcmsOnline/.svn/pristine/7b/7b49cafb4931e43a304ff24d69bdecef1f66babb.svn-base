package com.dnb.GcmsOnline.controller.test;

//import org.springframework.boot.autoconfigure.security.SecurityProperties.Headers;
import org.springframework.http.HttpEntity;
import org.springframework.http.HttpHeaders;
import org.springframework.http.HttpMethod;
import org.springframework.http.MediaType;
import org.springframework.http.ResponseEntity;
import org.springframework.web.client.RestTemplate;

import com.dnb.model.GcmsTextTypes;

public class GcmsTextTypesControllerTest {
	public void getAllTypes() {
		HttpHeaders headers = new HttpHeaders();
		headers.setContentType(MediaType.APPLICATION_JSON);
		 RestTemplate restTemplate = new RestTemplate();
		 String url = "http://localhost:8080/textTypes/alltypes";
		 HttpEntity<String> requestEntity = new HttpEntity<String>(headers);
		 ResponseEntity<GcmsTextTypes[]> responseEntity = restTemplate.exchange(url, HttpMethod.GET, requestEntity, GcmsTextTypes[].class);
		 GcmsTextTypes[] articles = responseEntity.getBody();
		  for(GcmsTextTypes article : articles) {
              System.out.println("Id:"+article.getGcmsTextTypeId()+", Category: "+article.getGcmsTextTypeName());
        }
	}
	
	public static void main(String args[]) {
		GcmsTextTypesControllerTest util = new GcmsTextTypesControllerTest();
		util.getAllTypes();
	}
}
